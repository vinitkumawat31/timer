{
	"manifest_version":2,
	"name": "Timer",
	"version": "1.0",
	"description": "Keeps a track of how long a tab has been open for.",
	"icons":{
		"128": "icon128.png",
		"48": "icon48.png",
		"16": "icon16.png"
	},
	"browser_action":{
		"default_icon": "icon16.png",
		"default_popup": "popup.html"
	},
	"background":{
		"scripts":["eventPage.js"],
		"persistent": false
	},
	"permissions": [
		"tabs",
		"storage"
	]

}